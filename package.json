{
  "name": "reinvest-app-common",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "tsup",
    "lint": "yarn eslint",
    "lint:fix": "yarn lint --fix",
    "generate": "graphql-codegen",
    "check-types": "tsc --pretty"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@hookooekoo/configs-tsconfig": "0.0.17",
    "@hookooekoo/eslint-config": "0.2.2",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@tanstack/react-query": "4.29.7",
    "@types/google-maps": "^3.2.3",
    "@types/node": "20.1.5",
    "@types/react": "18.2.6",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "graphql": "16.6.0",
    "graphql-request": "6.0.0",
    "lefthook": "1.3.13",
    "react": "^18.2.0",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "zod": "3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@tanstack/eslint-plugin-query/recommended",
      "@hookooekoo/eslint-config/next"
    ],
    "plugins": [
      "@tanstack/query"
    ],
    "rules": {
      "@tanstack/query/exhaustive-deps": 0,
      "@tanstack/query/prefer-query-object-syntax": "error"
    }
  },
  "prettier": "@hookooekoo/eslint-config/prettier.json"
}
